model TrinoDataSource {
  id             String     @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name           String
  host           String
  port           String
  catalog        String?
  username       String
  password       String?
  notes          String
  readOnly       Boolean    @default(true)
